{
  "name": "bloombeasts-web",
  "version": "1.0.0",
  "description": "Bloom Beasts - Web Deployment",
  "type": "module",
  "scripts": {
    "build": "rollup -c",
    "build:watch": "rollup -c -w",
    "dev": "concurrently \"npm run build:watch\" \"npm run serve\"",
    "serve": "cd ../.. && http-server -p 8000 -c-1"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "concurrently": "^8.2.2",
    "http-server": "^14.1.1",
    "rollup": "^4.9.6",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3"
  }
}
